%% title={Nome do artigo},
%% year={1995}
%% }

%% Exemplo de como colocar um artigo de jornal com autor
%% @ article{ref3,
author={Nome Autor},
address={São Paulo},
journal={A Folha de S.~Paulo},
month={7 set.},
pages={11-23},
title={Título do artigo},
year={1995}
}

%% Exemplo de como colocar um artigo de revista sem autor
%% @ article{ref4,
address={Rio de Janeiro},
journal={Nome da revista},
month={jan.},
number={35},
pages={51-60},
title={T\'{\i}tulo do Artigo},
year={1987}}

%% Mais um exemplo de como colocar um livro
@book{comer,
address={Rio de Janeiro -- RJ},
author={COMER, Douglas E.},
publisher={Editora Campus},
title={Interligaç\~{\a}o em rede com TCP/IP},
year={1998}}

%% Mais um exemplo de como colocar um livro, mas indicando as páginas.
@book{albitz,
pages={211 -- 236},
author={ALBITZ, Paul \& LIU, Cricket},
title={DNS and BIND},
year={1998}}

%% Mais um exemplo de como colocar um livro, mas com vários autores.
%% @ book{ref7,
author={X. Y. Z. Evans and Outro Autor and Mais Um and W. S. Caule},
title={Exemplo de citação no texto},
year={1987}}

%% Um exemplo de como citar um manual de algum grupo ou organização, obtido na internet.
@manual{rfc1034,
